{
  "name": "next2app",
  "version": "0.2.0",
  "type": "module",
  "bin": {
    "next2app": "./dist/bin/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/src/components/index.d.ts",
      "default": "./dist/src/components/index.js"
    }
  },
  "types": "./dist/src/components/index.d.ts",
  "scripts": {
    "build": "pnpm build:cli && pnpm build:components",
    "build:cli": "tsc -p tsconfig.cli.json && pnpm copy-data",
    "build:components": "tsc -p tsconfig.json",
    "dev": "pnpm copy-data && concurrently \"pnpm dev:cli\" \"pnpm dev:components\"",
    "dev:cli": "tsc -w -p tsconfig.cli.json",
    "dev:components": "tsc -w -p tsconfig.json",
    "copy-data": "pnpm copy-templates && pnpm copy-fastlane",
    "copy-templates": "copyfiles -u 3 \"src/cli/templates/**/*\" dist/templates",
    "copy-fastlane": "copyfiles -u 3 \"src/cli/fastlane/**/*\" dist/fastlane"
  },
  "peerDependencies": {
    "next": "^15.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@next2app/stack-navigation": "workspace:*",
    "@types/conf": "^3.0.3",
    "@types/dotenv": "^8.2.3",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer-autocomplete-prompt": "^3.0.3",
    "ansi-colors": "^4.1.3",
    "chalk": "^5.3.0",
    "classnames": "^2.5.1",
    "cli-progress": "^3.12.0",
    "commander": "^11.1.0",
    "conf": "^13.0.1",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.2.0",
    "get-port": "^7.1.0",
    "inquirer": "^9.2.12",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "ora": "^8.1.1",
    "progress": "^2.0.3",
    "qrcode-terminal": "^0.12.0",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@next2app/typescript-config": "workspace:*",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@types/cli-progress": "^3.11.6",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.10.0",
    "@types/progress": "^2.0.7",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "copyfiles": "^2.4.1",
    "jest": "^29.0.0",
    "next": "^15.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-jest": "^29.0.0",
    "typescript": "^5.3.0"
  }
}
